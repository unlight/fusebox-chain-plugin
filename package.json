{
  "name": "fusebox-chain-plugin",
  "version": "1.0.6",
  "description": "Custom chain plugin for FuseBox",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "clean": "rm -rf .fusebox lib",
    "compile": "tsc",
    "compile:w": "tsc -w",
    "benchmarks": "ts-node -F benchmarks/benchmark.ts",
    "pretest": "npm run compile",
    "test": "npm run t",
    "t": "npm run mocha -- src/*.spec.ts",
    "test:w": "npm run mocha -- --watch src/*.spec.ts ",
    "dev": "npm run compile && concurrently -r -k -p \"{name}:\" -n \"c,t\" \"npm run compile:w\" \"npm run test:w\"",
    "mocha": "mocha --compilers ts:ts-node/register",
    "release": "npm run clean && npm run compile"
  },
  "devDependencies": {
    "@types/del": "2.2.32",
    "@types/lodash": "^4.14.55",
    "@types/memory-fs": "0.3.0",
    "@types/mocha": "2.2.40",
    "@types/node": "7.0.8",
    "concurrently": "3.4.0",
    "del": "2.2.2",
    "fuse-box": "1.3.128",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "nperf": "^1.2.6",
    "pkg-dir": "1.0.0",
    "ts-node": "2.1.0",
    "typescript": "2.2.1"
  },
  "dependencies": {
    "memory-fs": "0.4.1",
    "lodash": "^4.17.4"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "fuse-box"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/fusebox-chain-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/fusebox-chain-plugin/issues"
  },
  "homepage": "https://github.com/unlight/fusebox-chain-plugin#readme"
}
