{
  "name": "fusebox-chain-plugin",
  "version": "0.0.0",
  "description": "Cache for chain plugins",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "clean": "rm -rf .fusebox lib",
    "compile": "tsc",
    "compile:w": "tsc -w",
    "test": "npm run t",
    "t": "npm run mocha -- src/*.spec.ts",
    "test:w": "npm run mocha -- --watch src/*.spec.ts ",
    "dev": "npm run compile && concurrently -r -k -p \"{name}:\" -n \"c,t\" \"npm run compile:w\" \"npm run test:w\"",
    "mocha": "mocha --compilers ts:ts-node/register"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.53",
    "@types/memory-fs": "^0.3.0",
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.5",
    "@types/through2": "2.0.32",
    "concurrently": "3.4.0",
    "espower-ts-node": "1.0.3",
    "fuse-box": "1.3.123",
    "memory-fs": "0.4.1",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "ts-node": "2.1.0",
    "typescript": "2.2.1"
  },
  "dependencies": {},
  "directories": {
    "test": "test"
  },
  "keywords": [
    "fuse-box"
  ],
  "license": "MIT"
}
