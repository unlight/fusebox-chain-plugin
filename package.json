{
  "name": "fusebox-chain-plugin",
  "version": "1.0.0",
  "description": "Cache for chain plugins",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "clean": "rm -rf .fusebox lib",
    "compile": "tsc",
    "compile:w": "tsc -w",
    "test": "npm run compile && npm run test:r",
    "test:r": "npm run mocha -- src/*.spec.ts",
    "test:w": "npm run mocha -- --watch src/*.spec.ts ",
    "dev": "concurrently -r -k -p \"{name}:\" -n \"c,t\" \"npm run compile:w\" \"npm run test:w\"",
    "mocha": "mocha --compilers ts:ts-node/register --require espower-ts-node"
  },
  "devDependencies": {
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.5",
    "@types/power-assert": "1.4.29",
    "@types/through2": "2.0.32",
    "@types/vinyl": "2.0.0",
    "concurrently": "3.3.0",
    "espower-ts-node": "1.0.3",
    "fuse-box": "1.3.122",
    "mocha": "3.2.0",
    "power-assert": "1.4.2",
    "ts-node": "2.1.0",
    "typescript": "2.2.1"
  },
  "dependencies": {},
  "directories": {
    "test": "test"
  },
  "keywords": [
    "fuse-box"
  ],
  "license": "MIT"
}
